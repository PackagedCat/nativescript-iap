{
  "name": "nativescript-iap",
  "version": "7.0.7",
  "description": "A NativeScript plugin for making in-app purchases",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "7.0.0",
      "ios": "7.0.0"
    },
		"hooks": [
			{
				"type": "after-prepare",
				"script": "scripts/after-prepare.js",
				"inject": true
			}
		]
  },
	"scripts": {
		"postinstall": "node postinstall-hooks.js",
		"preuninstall": "node preuninstall.js"
	},
  "repository": {
    "type": "git",
    "url": "https://github.com/PackagedCat/nativescript-iap"
  },
  "keywords": [
    "NativeScript",
    "iap",
    "in-app purchase",
    "in-app billing",
    "payment",
    "purchase",
    "billing",
    "subscription"
  ],
  "author": {
    "name": "Shamsuddin Dzhamalov",
    "email": "packagedcat@outlook.com"
  },
  "contributors": [],
  "license": "Apache-2.0",
	"dependencies": {
		"xcode": "^0.9.3"
	}
}
