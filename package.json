{
    "name": "nativescript-iap",
    "version": "8.0.3",
    "description": "A NativeScript plugin for making in-app purchases",
    "repository": {
        "type": "git",
        "url": "https://github.com/PackagedCat/nativescript-iap"
    },
    "scripts": {
        "clean": "git clean -f -X -d",
        "build": "tsc -b -v && cd ./src && cpy ./**/*.d.ts ../package/ --parents && cpy ../README.md ../package/",
        "setup": "npm run clean && npm install && ts-patch install"
    },
    "keywords": [
        "NativeScript",
        "iap",
        "in-app purchase",
        "in-app billing",
        "payment",
        "purchase",
        "billing",
        "subscription"
    ],
    "author": "Shamsuddin Dzhamalov <packagedcat@outlook.com>",
    "contributors": [],
    "license": "Apache-2.0",
    "devDependencies": {
        "@nativescript/android": "^8.1.1",
        "@nativescript/core": "^8.1.5",
        "@nativescript/types": "^8.1.0",
        "@nativescript/webpack": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "@vue/eslint-config-typescript": "^9.0.1",
        "cpy-cli": "^3.1.1",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3",
        "ts-node": "^9.0.0",
        "ts-patch": "^1.3.3",
        "typescript": "^4.4.4",
        "xcode": "^0.9.3"
    }
}
