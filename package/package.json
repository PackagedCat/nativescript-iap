{
    "name": "nativescript-iap",
    "version": "8.0.5",
    "description": "A NativeScript plugin for making in-app purchases",
    "main": "index",
    "typings": "index.d.ts",
    "nativescript": {
        "platforms": {
            "android": "8.0.0",
            "ios": "8.0.0"
        },
        "hooks": [
            {
                "type": "after-prepare",
                "script": "scripts/after-prepare.js",
                "inject": true
            }
        ]
    },
    "scripts": {
        "postinstall": "node scripts/postinstall.js",
        "preuninstall": "node scripts/preuninstall.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PackagedCat/nativescript-iap"
    },
    "keywords": [
        "NativeScript",
        "iap",
        "in-app purchase",
        "in-app billing",
        "payment",
        "purchase",
        "billing",
        "subscription"
    ],
    "author": {
        "name": "Shamsuddin Dzhamalov",
        "email": "packagedcat@outlook.com"
    },
    "contributors": [],
    "license": "Apache-2.0",
    "dependencies": {
        "xcode": "^3.0.1"
    }
}
